{
    "version": 2,
    "name": "payzen-frontend",
    "builds": [
      {
        "src": "apps/frontend/package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "apps/frontend/$1"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ]
  }
  